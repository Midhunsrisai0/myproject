{
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myproject/wsgi.py"
    }
  ],
  "version": 2,
  "routes": [
    { "src": "/api/initialize", "dest": "/api/initialize.js" },
    { "src": "/(.*)", "dest": "/myproject/wsgi.py" }
  ]
  
}
